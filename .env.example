# .env.example - Environment variables for mic project

# Database Configuration
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mic_database
DB_USER=mic_user
DB_PASSWORD=mic_password

# API Keys
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY
OPENAI_API_KEY=YOUR_OPENAI_API_KEY
RAZORPAY_KEY_ID=YOUR_RAZORPAY_KEY_ID
RAZORPAY_KEY_SECRET=YOUR_RAZORPAY_KEY_SECRET

# Application Settings
APP_SECRET_KEY=YOUR_APP_SECRET_KEY
DEBUG_MODE=False
LOG_LEVEL=INFO

# LLM Configuration (if applicable)
LLAMA_CPP_MODEL_PATH=/path/to/your/llama-cpp-model.gguf
HUGGINGFACE_TOKEN=YOUR_HUGGINGFACE_TOKEN

# Other services
# Add any other API keys or service credentials here
# EXAMPLE_SERVICE_API_KEY=YOUR_EXAMPLE_SERVICE_API_KEY
